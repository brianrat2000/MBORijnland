{% extends 'base.html.twig' %}

{% block title %}Category{% endblock %}

{% block body %}
    <h1>Category - {{ category.Name }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ category.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ category.Name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ category.Description }}</td>
            </tr>
        </tbody>
    </table>

{% endblock %}
